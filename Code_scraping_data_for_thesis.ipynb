    {
      "cell_type": "code",
      "source": [
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "from urllib.parse import urljoin\n",
        "from pathlib import Path\n",
        "from google.colab import drive\n",
        "import time\n",
        "\n",
        "# Mount Google Drive\n",
        "drive.mount('/content/drive')\n",
        "\n",
        "# Function to retrieve text from a given URL\n",
        "def retrieve_text(url, file_name, retries=1):\n",
        "    try:\n",
        "        # Send a GET request to the URL\n",
        "        response = requests.get(url)\n",
        "\n",
        "        # Raise an exception if the request was unsuccessful\n",
        "        response.raise_for_status()\n",
        "\n",
        "        # Parse the HTML content using BeautifulSoup\n",
        "        soup = BeautifulSoup(response.content, 'html.parser')\n",
        "\n",
        "        # Extract the text content\n",
        "        text = soup.get_text()\n",
        "\n",
        "        # Create a file path to save the text file\n",
        "        file_path = f'/content/drive/MyDrive/Barbara + David/Thesis data/{file_name}.txt'\n",
        "\n",
        "        # Save the text as a .txt file\n",
        "        with open(file_path, 'w') as file:\n",
        "            file.write(text)\n",
        "\n",
        "        print(f'Saved text as {file_name}.txt')\n",
        "    except requests.exceptions.RequestException as e:\n",
        "        if isinstance(e, requests.exceptions.HTTPError) and e.response.status_code == 504 and retries > 0:\n",
        "            # Retry the request after a short delay if Gateway Timeout error occurs\n",
        "            print(f'Retrying request after a delay for {file_name}...')\n",
        "            time.sleep(5)\n",
        "            retrieve_text(url, file_name, retries-1)\n",
        "        else:\n",
        "            print(f'Error retrieving text for {file_name}: {e}')\n",
        "\n",
        "# Function to recursively follow nested links\n",
        "def follow_links(url):\n",
        "    try:\n",
        "        # Send a GET request to the URL\n",
        "        response = requests.get(url)\n",
        "\n",
        "        # Raise an exception if the request was unsuccessful\n",
        "        response.raise_for_status()\n",
        "\n",
        "        # Parse the HTML content using BeautifulSoup\n",
        "        soup = BeautifulSoup(response.content, 'html.parser')\n",
        "\n",
        "        # Find all <a> tags within <li> tags\n",
        "        links = soup.select('li > a[href]')\n",
        "\n",
        "        # Iterate over the links\n",
        "        for link in links:\n",
        "            href = link['href']\n",
        "\n",
        "            # Extract the identifier from the href\n",
        "            identifier = href.strip('/c/cme/')\n",
        "\n",
        "            # Construct the full URL of the link\n",
        "            full_url = urljoin(url, href)\n",
        "\n",
        "            # Send a GET request to the link\n",
        "            link_response = requests.get(full_url)\n",
        "\n",
        "            # Raise an exception if the request was unsuccessful\n",
        "            link_response.raise_for_status()\n",
        "\n",
        "            # Parse the HTML content of the link\n",
        "            link_soup = BeautifulSoup(link_response.content, 'html.parser')\n",
        "\n",
        "            # Find the \"View entire text\" link\n",
        "            view_link = link_soup.find('a', href=lambda href: href and 'page=viewtextnote;rgn=full+text' in href)\n",
        "\n",
        "            if view_link:\n",
        "                view_href = view_link['href']\n",
        "\n",
        "                # Construct the full URL of the \"View entire text\" link\n",
        "                full_view_url = urljoin(url, view_href)\n",
        "\n",
        "                # Send a GET request to the \"View entire text\" link\n",
        "                view_response = requests.get(full_view_url)\n",
        "\n",
        "                # Raise an exception if the request was unsuccessful\n",
        "                view_response.raise_for_status()\n",
        "\n",
        "                # Parse the HTML content of the \"View entire text\" link\n",
        "                view_soup = BeautifulSoup(view_response.content, 'html.parser')\n",
        "\n",
        "                # Find the \"View entire text now\" link\n",
        "                nested_link = view_soup.find('a', href=lambda href: href and 'rgn=main;view=fulltext' in href)\n",
        "\n",
        "                if nested_link:\n",
        "                    nested_href = nested_link['href']\n",
        "\n",
        "                    # Construct the full URL of the nested link\n",
        "                    full_nested_url = urljoin(url, nested_href)\n",
        "\n",
        "                    # Retrieve the text from the nested link\n",
        "                    retrieve_text(full_nested_url, identifier)\n",
        "                else:\n",
        "                    print(f'Nested link not found for {identifier}')\n",
        "            else:\n",
        "                print(f'View entire text link not found for {identifier}')\n",
        "\n",
        "            # Introduce a delay to avoid overwhelming the server\n",
        "            time.sleep(1)\n",
        "    except requests.exceptions.RequestException as e:\n",
        "        print(f'Error following links for {url}: {e}')\n",
        "\n",
        "# Specify the URL of the main page\n",
        "url = 'https://quod.lib.umich.edu/c/cme/browse.html'\n",
        "\n",
        "# Start following links from the main page\n",
        "follow_links(url)\n"
      ],
